algoritmo "semnome"
var
   contador, soma : Inteiro
inicio

      contador <- 6
      soma <- contador
      
      Enquanto (contador <= 100) faca
               Escreva ( contador)
               contador <- contador + 2
               soma <- soma + contador
      FimEnquanto
      
      Escreva (" Acabou! A soma dos valores é igual a",soma,".")

fimalgoritmo