algoritmo "semnome"
var
   numero, soma, resultado: Inteiro
inicio
      Repita
            Escreva ("Digite um número: ")
            Leia (numero)
            soma <- soma + numero
      Ate (numero = 1111)
      
      resultado <- soma - 1111

      Escreva ("A soma dos valores digitados é igual a",resultado,".")
fimalgoritmo