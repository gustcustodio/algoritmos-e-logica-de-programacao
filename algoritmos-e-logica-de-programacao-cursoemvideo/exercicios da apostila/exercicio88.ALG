algoritmo "Exercício88"
var
   C : Caracter

procedimento Gerador(F: Caracter)
var
   I: Inteiro
inicio
      para I <- 1 ate 4 faca
      EscrevaL (F)
      fimpara
fimprocedimento

inicio
      C <- ("Aprendendo Portugol")
      EscrevaL ("+-------=======------+")
      Gerador(C)
      EscrevaL ("+-------=======------+")
fimalgoritmo